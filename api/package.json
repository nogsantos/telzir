{
	"name": "tapi-elzir",
	"version": "0.1.0",
	"description": "Telzir Fale mais",
	"scripts": {
		"test": "jest --verbose --no-cache",
		"test:watch": "jest --verbose --no-cache --watch",
		"start": "npm-run-all --parallel build:*",
		"build:server": "nodemon ./src/server.js"
	},
	"main": "./src/server.js",
	"author": "Fabricio Nogueira",
	"license": "ISC",
	"private": true,
	"keywords": [
		"vizir",
		"challange",
		"telzir",
		"api",
		"node"
	],
	"dependencies": {
		"body-parser": "1.18.3",
		"cors": "2.8.5",
		"dotenv": "^8.2.0",
		"express": "4.17.3",
		"module-alias": "2.1.0",
		"mongoose": "5.8.0",
		"nodemon": "^2.0.4",
		"npm-run-all": "4.1.5"
	},
	"devDependencies": {
		"eslint": "5.12.1",
		"eslint-plugin-jest": "22.2.2",
		"jest": "^26.1.0",
		"mockingoose": "2.12.0"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:nogsantos/telzir.git"
	},
	"bugs": {
		"url": "https://github.com/nogsantos/telzir/issues"
	},
	"engines": {
		"node": "12.x"
	},
	"_moduleAliases": {
		"@root": ".",
		"@config": "src/config",
		"@routes": "src/routes",
		"@modules": "src/modules"
	},
	"jest": {
		"verbose": true,
		"testPathIgnorePatterns": [
			"<rootDir>/static/",
			"<rootDir>/node_modules/",
			"<rootDir>/package.json"
		],
		"moduleNameMapper": {
			"@modules(.*)$": "<rootDir>/src/modules/$1",
			"@config(.*)$": "<rootDir>/src/config/$1"
		},
		"modulePaths": [
			"<rootDir>"
		],
		"testEnvironment": "node"
	}
}
